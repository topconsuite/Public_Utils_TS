{
  "name": "@npm-telluria-tecnologia/telluria-utils",
  "version": "1.0.1",
  "description": "important and useful functions for application development",
  "main": "lib",
  "types": "lib",
  "scripts": {
    "build": "yarn test && tsc -p .",
    "pubnpm": "yarn test && tsc -p . && yarn publish --no-git-tag-version",
    "commit": "git-cz",
    "lint-check": "eslint src --ext js,ts,tsx",
    "lint-fix": "eslint src --fix --ext js,ts,tsx",
    "prepare": "husky install",
    "test": "jest",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@types/jest": "^27.0.2",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.24.2",
    "husky": "^7.0.2",
    "jest": "^27.2.5",
    "lint-staged": "^11.2.3",
    "pinst": "^2.1.6",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "yarn lint-fix",
      "yarn lint-check"
    ]
  }
}
